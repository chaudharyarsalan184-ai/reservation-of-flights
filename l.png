    <script>
      // ==== BACKEND DATA SIMULATION (runtime update bhi possible) ====
      const backendMetaData = {
        title: "Fareskart.us - Best Flight Deals",
        description: "Book cheap flights with Fareskart.us at lowest fares.",
        keywords: "flights, cheap flights, fareskart, airlines booking",
        author: "Fareskart Team",

        ogTitle: "Fareskart.us - Book Flights Easily",
        ogDescription:
          "Fareskart.us helps you book cheap flights across the world.",
        ogImage: "https://yourcdn.com/og-image.jpg",

        twitterCard: "summary_large_image",
        twitterSite: "@fareskart",
        twitterImage: "https://yourcdn.com/twitter-image.jpg",
      };

      // ==== FRONTEND CODE to dynamically re-apply meta (SPA navigation) ====
      function applyMeta(meta) {
        if (meta.title) document.title = meta.title;

        if (meta.description)
          document
            .querySelector('meta[name="description"]')
            .setAttribute("content", meta.description);

        if (meta.keywords)
          document
            .querySelector('meta[name="keywords"]')
            .setAttribute("content", meta.keywords);

        if (meta.author)
          document
            .querySelector('meta[name="author"]')
            .setAttribute("content", meta.author);

        if (meta.ogTitle)
          document
            .querySelector('meta[property="og:title"]')
            .setAttribute("content", meta.ogTitle);

        if (meta.ogDescription)
          document
            .querySelector('meta[property="og:description"]')
            .setAttribute("content", meta.ogDescription);

        if (meta.ogImage)
          document
            .querySelector('meta[property="og:image"]')
            .setAttribute("content", meta.ogImage);

        if (meta.twitterCard)
          document
            .querySelector('meta[name="twitter:card"]')
            .setAttribute("content", meta.twitterCard);

        if (meta.twitterSite)
          document
            .querySelector('meta[name="twitter:site"]')
            .setAttribute("content", meta.twitterSite);

        if (meta.twitterImage)
          document
            .querySelector('meta[name="twitter:image"]')
            .setAttribute("content", meta.twitterImage);
      }

      // ‚úÖ Page load hone ke baad bhi meta update hoga (SPA ke liye)
      window.addEventListener("DOMContentLoaded", () =>
        applyMeta(backendMetaData)
      );
    </script>
